{
  "name": "e-commerce",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "e-commerce",
  "sourceRoot": "apps/e-commerce/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/e-commerce",
        "index": "apps/e-commerce/src/index.html",
        "browser": "apps/e-commerce/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/e-commerce/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/e-commerce/src/favicon.ico",
          "apps/e-commerce/src/assets"
        ],
        "styles": ["apps/e-commerce/src/styles.scss"],
        "scripts": [],
        "server": "apps/e-commerce/src/main.server.ts",
        "prerender": true,
        "ssr": {
          "entry": "apps/e-commerce/server.ts"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "e-commerce:build:production"
        },
        "development": {
          "buildTarget": "e-commerce:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "e-commerce:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/e-commerce/**/*.ts",
          "apps/e-commerce/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/e-commerce/jest.config.ts"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "e-commerce:build",
        "staticFilePath": "dist/apps/e-commerce/browser"
      }
    }
  }
}
